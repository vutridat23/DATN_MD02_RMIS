        <activity
            android:name=".ui.thanhtoan.ThanhToanActivity"
            android:exported="false" />
        <activity
            android:name=".ui.auth.OnboardingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.auth.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.phucvu.OrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.thungan.ThuNganActivity"
            android:exported="false" />
        <activity
            android:name=".ui.thungan.InvoiceActivity"
            android:exported="false" />
        <activity
            android:name=".ui.thungan.PrintBillActivity"
            android:exported="false" />
        <activity
            android:name=".ui.revenue.RevenueActivity"
            android:exported="false" />
        <activity
            android:name=".ui.bep.BepActivity"
            android:exported="false" />
    </application>

</manifest>